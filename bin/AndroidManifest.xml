<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fugitahyodo.saddm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".SaddmApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.fugitahyodo.saddm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.fugitahyodo.saddm.GerenciadorPrincipal"></activity>
        <activity android:name="com.fugitahyodo.saddm.GerenciadorCadastro"></activity>
        <activity android:name="com.fugitahyodo.saddm.GerenciadorCadastroChaveAleatoria"></activity>
        <activity android:name="br.com.saddm.gerenciador.GerenciadorGeracaoChaves"></activity>
		<activity android:name="com.fugitahyodo.saddm.SaddmActivity"></activity>
		<activity android:name="com.fugitahyodo.saddm.Signer"></activity>
		<activity android:name="com.fugitahyodo.saddm.Verify"></activity>  
		<activity android:name="br.com.thinkti.android.filechooser.FileChooser"></activity>  
    </application>

</manifest>